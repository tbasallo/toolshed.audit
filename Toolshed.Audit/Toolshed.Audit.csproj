<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RepositoryUrl>https://github.com/tbasallo/toolshed.audit</RepositoryUrl>
    <PackageProjectUrl>https://github.com/tbasallo/toolshed.audit</PackageProjectUrl>
    <RepositoryType>Git</RepositoryType>
    <Description>A simple auditing framework that uses Azure queues and tables</Description>
    <Version>2.0.4</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.4.0" />
    <PackageReference Include="Azure.Storage.Common" Version="12.3.0" />
    <PackageReference Include="Azure.Storage.Queues" Version="12.11.0" />
    <PackageReference Include="Microsoft.Azure.Cosmos.Table" Version="1.0.6" />
    <PackageReference Include="System.Text.Json" Version="4.7.1" />
    <PackageReference Include="Toolshed" Version="7.1.0.17" />
  </ItemGroup>
</Project>
